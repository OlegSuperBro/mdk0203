   "source": [
    "PLANCK_CONSTANT = 6.626e-34\n",
    "SPEED_OF_LIGHT = 3.0e8\n",
    "AVOGADRO_CONSTANT = 6.022e23\n",
    "\n",
    "def get_wv(d0):\n",
    "    molecule_energy = (d0 * 1e3) / AVOGADRO_CONSTANT\n",
    "    wavelength_m = PLANCK_CONSTANT * SPEED_OF_LIGHT / molecule_energy\n",
    "    return wavelength_m * 1e9\n",
    "\n",
    "print(get_wv(497))\n"
   ]
  }
